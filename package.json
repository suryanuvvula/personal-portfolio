{
  "name": "portfolio-surya-nuvvula",
  "version": "1.0.0",
  "description": "Surya Nuvvula's Professional Portfolio",
  "scripts": {
    "install:all": "cd client && npm install && cd ../server && npm install",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm run build && cd ../server && npm run build"
  },
  "keywords": ["portfolio", "react", "typescript", "express", "nodejs"],
  "author": "Surya Nuvvula",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
